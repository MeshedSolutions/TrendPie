@model IEnumerable<TrendPie.Models.Campaign>

<div class="clearme"></div>
<div>
    @foreach (var campaign in Model)
    {
        <div class="campaignCloseItemWrap">
            <div class="ce_Wrap">
                <div class="campaignName">
                    @campaign.Name
                </div>
                <div class="campaignEarnings">
                    <span class="timeLable">Earned:</span> $150
                </div>
            </div>
            <div class="campaignEndWrap">
                <div class="campaignEndDay">
                    @campaign.EndDay
                </div>
                <div class="campaignEndMonth">
                    @campaign.EndMonthAbbreviation
                </div>
            </div>
            <div class="clearme"></div>
            <div class="space15"></div>
        </div>
    }
</div>
<a href="/Campaigns/Complete">
    <div class="bttnViewAllWrap">
        <div class="bttnViewAllBlu">
            VIEW All
            <div class="bttnViewAllArrow"></div>
        </div>
    </div>
</a>
<div class="clearme"></div>
<div class="space25"></div>
